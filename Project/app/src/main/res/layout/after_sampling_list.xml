<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#191F36">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Registration deadline has passsed\nSampling is done!"
        tools:layout_editor_absoluteX="152dp"
        tools:layout_editor_absoluteY="99dp"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/buttonWaitlistEntrants"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <Button
        android:id="@+id/buttonWaitlistEntrants"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#0A4753"
        android:text="Entrants in the waitlist"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/buttonSelectedEntrants"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <Button
        android:id="@+id/buttonSelectedEntrants"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Selected entrants"
        android:backgroundTint="#0A4753"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/buttonCancelledEntrants"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonWaitlistEntrants"/>

    <Button
        android:id="@+id/buttonCancelledEntrants"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cancelled Entrants"
        android:backgroundTint="#0A4753"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/buttonChosenEntrants"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonSelectedEntrants"/>

    <Button
        android:id="@+id/buttonChosenEntrants"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Final chosen entrants"
        android:backgroundTint="#0A4753"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonCancelledEntrants"/>
</androidx.constraintlayout.widget.ConstraintLayout>